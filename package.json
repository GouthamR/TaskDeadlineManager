{
  "name": "taskdeadlinemanager",
  "version": "1.0.0",
  "description": "An app to help manage associated tasks and deadlines.",
  "main": "app.js",
  "engines": {
    "node": "6.11.1"
  },
  "scripts": {
    "start": "node app.js",
    "start-dev": "nodemon app.js",
    "build": "tsc -p tsconfig-files/tsconfig-main.json && tsc -p tsconfig-files/tsconfig-landing.json && handlebars views/templates/ -f public/js/handlebars-templates.js",
    "build-dev": "concurrently \"tsc -w -p tsconfig-files/tsconfig-main.json\" \"tsc -w -p tsconfig-files/tsconfig-landing.json\" \"npm run handlebars-dev\"",
    "handlebars-dev": "watch \"handlebars views/templates/ -f public/js/handlebars-templates.js\" views/templates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GouthamR/TaskDeadlineManager.git"
  },
  "author": "GouthamR",
  "bugs": {
    "url": "https://github.com/GouthamR/TaskDeadlineManager/issues"
  },
  "homepage": "https://github.com/GouthamR/TaskDeadlineManager#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "connect-mongodb-session": "^1.3.0",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.0.1",
    "mongodb": "^2.2.16",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "@types/handlebars": "^4.0.31",
    "@types/jquery": "^2.0.34",
    "concurrently": "^3.5.1",
    "handlebars": "^4.0.6",
    "nodemon": "^1.12.1",
    "typescript": "^2.1.5",
    "watch": "^1.0.2"
  }
}
